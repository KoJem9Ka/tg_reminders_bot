{
  "name": "tg_reminders_bot",
  "version": "0.0.1",
  "description": "KoJem9Ka reminders telegram bot",
  "author": "KoJem9Ka",
  "private": true,
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "dotenv -c dev -- nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:prod:migrate": "dotenv -c prod -- prisma migrate deploy",
    "prisma:prod:seed": "dotenv -c prod -- prisma db seed",
    "prisma:dev:reset": "dotenv -c dev -- prisma migrate reset",
    "prisma:dev:migrate": "dotenv -c dev -- prisma migrate dev",
    "prisma:dev:migrate:create": "dotenv -c dev -- prisma migrate dev --create-only",
    "prisma:dev:push": "dotenv -c dev -- prisma db push",
    "prisma:dev:pull": "dotenv -c dev -- prisma db pull",
    "prisma:dev:seed": "dotenv -c dev -- prisma db seed",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.6",
    "@nestjs/core": "^10.2.6",
    "@nestjs/platform-express": "^10.2.6",
    "@prisma/client": "^5.3.1",
    "@telegraf/session": "2.0.0-beta.6",
    "chrono-node": "^2.7.0",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.10",
    "joi": "^17.10.2",
    "kysely": "^0.23.4",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "nestjs-telegraf": "^2.7.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "telegraf": "^4.14.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.199",
    "@types/ms": "^0.7.31",
    "@types/node": "^20.7.0",
    "@types/pg": "^8.10.3",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "jest": "^29.7.0",
    "prisma": "^5.3.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^4.3.1",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed"
  }
}
